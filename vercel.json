{
  "version": 2,
  "name": "manesbymiriam",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/policies.html",
      "dest": "/policies.html"
    },
    {
      "src": "/policies",
      "dest": "/policies.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
